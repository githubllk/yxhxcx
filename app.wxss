page{
  background-color: #F9F9F9;
}
.space-between{
    height: 15rpx;
}

/*  自定义tabbar样式*/
 
.tabbar {
  display: flex;
  flex-direction: row;
  position: fixed;
  width: 100%;
  bottom: 0;
  z-index: 99999;
}
 
.tabbar-item {
  flex-grow: 1;
  padding: 6rpx 0;
  text-align: center;
}
 
.tabbar-icon {
  width: 46rpx;
  height: 46rpx;
  display: block;
  margin: 0 auto;
}
 
.tabbar-text {
  font-size: 24rpx;
  /* color: #515050; */
}
/* z字体颜色A3 */
.font-color-A3{
  color: #A3A3A3;
}
.white-bgc{
  background-color: #fff;
}

/* 选择组件 */
.picker-view {
  height: 376rpx;
  width: 100%;
}

picker-view {
  background-color: #fff;
  padding: 0;
  width: 100%;
  height: 376rpx;
  bottom: 0;
  position: fixed;
  z-index: 9999;
}

picker-view-column view {
  vertical-align: middle;
  font-size: 34rpx;
  line-height: 24rpx;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.shadow {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0rpx;
  background-color: #0000004D;
}
/* 增加底部高度 */
.blank{
  height: 150rpx;
}
.price{
  font-size: 32rpx;
  color:#F63256;
}