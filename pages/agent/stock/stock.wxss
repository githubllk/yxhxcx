/* pages/agent/stock/stock.wxss */
.stock-list {
    background-color: #fff;
    display: flex;
    height: 160rpx;
    border-bottom: 2rpx #F5F5F5 solid;
    justify-content : space-between;
}
.stock-left {
    width: 694rpx;
    margin-left: 20rpx;
    margin-right: 10rpx;
}
.stock-top {
    margin-top: 20rpx;
    display: flex;
    justify-content : space-between;
}

.stock-name {
    min-width: 128rpx;
}

.stock-goods {
    min-width: 130rpx;
}

.stock-right {
    line-height: 160rpx;
    margin-right: 30rpx;
}
.stock-right image {
    margin-left: 10rpx;
    width: 25rpx;
    height: 25rpx;
}

.stock-bottom {
    margin-top: 20rpx;
    display: flex;
    justify-content : space-between;

}
.stock-phone {
    color: #A3A3A3;
}

.stock-num {
    color: #B79547;
}
.foot{
    display: flex;
    position: fixed;
    bottom: 0rpx;
    justify-content: space-between;
    width: 100%;
    height: 98rpx;
    text-align: center;
    border-top: 2rpx #eeeeee solid;
}
.log{
    width: 50%;
    line-height: 98rpx;
    color: #B79547;
}
.ruku{
    width: 50%;
    line-height: 98rpx;
    color: #fff;
    background-image: linear-gradient(to right, #bda54f, #9e722d);
}